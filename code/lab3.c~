#include<stdio.h>
int main(){

	int n;
	int i=0;
	int j=0;
	int a[5][5];
	for(i=0;i<5;i++)
		for(j=0;j<5;j++)
			scanf("%d",&a[i][j]);
	int e[25];
	int count=0;

	
	int left=0;
	int right=0;
	for(i=0;i<5;i++)
	{
		right=a[i][0]+a[i][1]+a[i][2]+a[i][3]+a[i][4];
		left=0;

		for(j=0;j<5;j++)
			{if(left==right)
				e[count++]=a[i][j];
								
			left+=a[i][j];
			right-=a[i][j];
		
		
		}
		}	
		
printf("%d",&count);

for(i=0;i<count;i++)
	printf("%d  ",e[i]);


return(0);

}


